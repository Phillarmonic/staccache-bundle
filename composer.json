{
    "name": "phillarmonic/staccache-bundle",
    "description": "A Symfony bundle for entity caching with locking support",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Phillarmonic Software",
            "email": "phillarmonicsoftware@outlook.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "symfony/framework-bundle": "^6.0|^7.0",
        "symfony/lock": "^6.0|^7.0",
        "symfony/serializer": "^6.0|^7.0",
        "symfony/property-access": "^6.0|^7.0",
        "symfony/form": "^7.2",
        "doctrine/orm": "^2.0|^3.0",
        "doctrine/doctrine-bundle": "^2.13",
        "predis/predis": "^1.1|^2.0",
        "monolog/monolog": "^2.0|^3.0",
        "ext-redis": "*"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": "^9.5|^10.0|^11.0",
        "symfony/phpunit-bridge": "^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Phillarmonic\\StaccacheBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phillarmonic\\StaccacheBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^6.0|^7.0"
        }
    }
}
